<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">		<xs:element name="purchaseOrder">      <xs:complexType>         <xs:group ref="purchaseOrderData"/>               </xs:complexType>		</xs:element>		<xs:group name="purchaseOrderData">		<xs:sequence>		 <xs:element ref="shipTo" />		  <xs:element ref="billTo" />		   <xs:element ref="items" />		    <xs:element ref="ship_date" />		     <xs:element ref="ship_method" />		</xs:sequence>	</xs:group>		<xs:group name="listBillTo">      <xs:sequence>      <xs:element name="name" type="xs:string"></xs:element>      <xs:element name="street" type="xs:string"></xs:element>      <xs:element name="city" type="xs:string"></xs:element>      <xs:element name="state" type="xs:string"></xs:element>      <xs:element name="zip" type="xs:string"></xs:element>      </xs:sequence> 		</xs:group>    <xs:element name="shipTo">    <xs:complexType>      <xs:group ref="listBillTo"/>      <xs:attribute name="country" type="xs:string"></xs:attribute>         </xs:complexType>    </xs:element>			<xs:element name="billTo">	  <xs:complexType>	   <xs:group ref="listBillTo"/>	     <xs:attribute name="country" type="xs:string"></xs:attribute>	  </xs:complexType>	</xs:element>		<xs:group name="listItem">		<xs:all>		<xs:element name="productName" type="xs:string"></xs:element>		<xs:element name="quantity" type="xs:integer"></xs:element>		<xs:element name="USPrice" type="xs:decimal"></xs:element>		</xs:all>		</xs:group>		<xs:element name="items">	<xs:complexType>		<xs:sequence>			<xs:element name="item" minOccurs="1" maxOccurs="unbounded">			<xs:complexType>			<xs:group ref="listItem" />						<xs:attribute name="partNum" type="xs:string" />				</xs:complexType>		</xs:element>		</xs:sequence>	</xs:complexType>	</xs:element>			<xs:element name="ship_date" type="xs:string"/>	<xs:element name="ship_method">	   <xs:complexType>	     <xs:attribute name="type" type="xs:string"></xs:attribute>	   </xs:complexType>	</xs:element>			</xs:schema>